<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.itson.thecheezery">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="Molina.Arenas.thecheezery.combos"></activity>
        <activity android:name="Molina.Arenas.thecheezery.salties" />
        <activity android:name="Molina.Arenas.thecheezery.sweets" />
        <activity android:name="Molina.Arenas.thecheezery.hot_drinks" />
        <activity android:name="Molina.Arenas.thecheezery.ProductosActivity" />
        <activity android:name="Molina.Arenas.thecheezery.MenuActivity" />
        <activity android:name="Molina.Arenas.thecheezery.RegisterActivity" />
        <activity android:name="Molina.Arenas.thecheezery.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>